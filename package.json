{
  "name": "eos-solutions-react",
  "version": "0.1.0",
  "description": "The main page of EOS UX/UI Solutions.",
  "private": false,
  "homepage": "https://brand.eosdesignsystem.com/",
  "dependencies": {
    "@reach/router": "^1.2.1",
    "@testing-library/jest-dom": "^5.5.0",
    "@testing-library/react": "^10.0.4",
    "@testing-library/user-event": "^10.1.0",
    "axios": "^0.19.2",
    "eos-icons": "^3.1.0",
    "i18next": "^19.3.4",
    "node-sass": "^4.13.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-i18next": "^11.3.4",
    "react-scripts": "3.4.1"
  },
  "devDependencies": {
    "eslint-config-prettier": "^6.10.1",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^4.2.5",
    "prettier": "^2.0.5",
    "sass-lint": "^1.12.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "test:sass": "sass-lint -v -q",
    "format": "prettier --write .",
    "js:fix": "eslint src/ --fix",
    "test:js": "eslint src/"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test:sass && npm run test:js"
    }
  },
  "keywords": [
    "open source",
    "ux ui",
    "eos",
    "user experience"
  ],
  "author": "EOS UX/UI Solutions",
  "license": "MIT",
  "bugs": {
    "url": "https://gitlab.com/SUSE-UIUX/eos-landing-react/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.com:SUSE-UIUX/eos-landing-react.git"
  }
}
